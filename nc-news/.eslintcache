oders/Frontend/nc-news/nc-news/src/Components/Navbar.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/User.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/ArticlesList.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/TopicsList.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/Article.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/Api.js",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/ArticlesCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/ArticleCard.jsx",["109"],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/Loading.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/ArticleCommentsCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/TopicsCard.jsx",["110"],"import React, { useState, useEffect } from 'react';\nimport Loading from './Loading';\nimport TopicsArticlesCard from './TopicsArticlesCard';\nimport '../CSS/Topics.css';\nimport { fetchArticles } from './Api';\n\nconst TopicsCard = ({ slug, description }) => {\n  const [numberOfArticles, setNumberOfArticles] = useState(0);\n  const [showArticles, setShowArticles] = useState(false);\n  const [articles, setArticles] = useState([]);\n  const [loading, setLoading] = useState(true);\n  const [query, setQuery] = useState({ sort_by: 'votes', order: 'desc' });\n\n  useEffect(() => {\n    fetchArticles(query).then((data) => {\n      const filterData = data.filter((article) => article.topic === slug);\n      setArticles(filterData);\n      setNumberOfArticles(filterData.length);\n      setLoading(false);\n    });\n  }, [query, slug]);\n\n  return (\n    <li className=\"topicsCard\">\n      <h3>{slug}</h3>\n      <p className=\"topicsBody\">{description}</p>\n      <p className=\"topicsArticlesCount\">\n        Contains {`${numberOfArticles}`} articles.{' '}\n        <button\n          type=\"button\"\n          className=\"topicsButton\"\n          onClick={() => setShowArticles(!showArticles)}\n        >\n          {showArticles ? (\n            <p className=\"buttonText\">Hide Articles</p>\n          ) : (\n            <p className=\"buttonText\">Show Articles</p>\n          )}\n        </button>\n      </p>\n      <section id=\"topicsArticlesCard\">\n        <ul id=\"articlesContent\">\n          {showArticles ? (\n            loading ? (\n              <Loading item=\"articles\" />\n            ) : (\n              articles.map((article) => {\n                return (\n                  <TopicsArticlesCard key={article.article_id} {...article} />\n                );\n              })\n            )\n          ) : (\n            <></>\n          )}\n        </ul>\n      </section>\n    </li>\n  );\n};\n\nexport default TopicsCard;\n","/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/TopicsArticlesCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/UserCard.jsx",["111","112","113"],"import React, { useState, useEffect } from 'react';\nimport '../CSS/User.css';\nimport { fetchComments, fetchArticles } from './Api';\nimport UserArticlesCard from './UserArticlesCard';\nimport UserCommentsCard from './UserCommentsCard';\nimport Loading from './Loading';\n\nconst UserCard = (user) => {\n  const { username, avatar_url, name } = user[0];\n\n  const [articles, setArticles] = useState([]);\n  const [showArticles, setShowArticles] = useState(false);\n  const [comments, setComments] = useState([]);\n  const [showComments, setShowComments] = useState(false);\n  const [articlesLoading, setArticlesLoading] = useState(true);\n  const [commentsLoading, setCommentsLoading] = useState(true);\n  const [articlesQuery, setArticlesQuery] = useState({\n    sort_by: 'created_at',\n    order: 'desc',\n  });\n  const [commentsQuery, setCommentsQuery] = useState({\n    sort_by: 'created_at',\n    order: 'desc',\n  });\n\n  useEffect(() => {\n    fetchArticles(articlesQuery)\n      .then((data) => {\n        const filterArticles = data.filter(\n          (article) => article.author === username\n        );\n        setArticles(filterArticles);\n        setArticlesLoading(false);\n        return filterArticles;\n      })\n      .then((filterData) => {\n        return filterData.map((article) => {\n          return fetchComments(article.article_id, commentsQuery);\n        });\n      })\n      .then((response) => {\n        return Promise.all(response);\n      })\n      .then((comments) => {\n        return comments.map((article) => {\n          const correctComments = article.filter((comment) => {\n            return comment.author === username;\n          });\n\n          return correctComments;\n        });\n      })\n      .then((correctedComments) => {\n        const actualComments = correctedComments.filter(\n          (article) => article.length > 0\n        );\n\n        const comments = actualComments.flat();\n\n        setComments(comments);\n        setCommentsLoading(false);\n      });\n  }, [username]);\n\n  return (\n    <section id=\"user\">\n      <h3>{username}</h3>\n      <img src={avatar_url} alt={`${username}'s avatar.`} />\n      <p>Name: {name}</p>\n      <ul>\n        Published:\n        <li>\n          {articlesLoading\n            ? 'Calculating articles... '\n            : `${articles.length} articles. `}\n          <button\n            type=\"button\"\n            className=\"userButton\"\n            onClick={() => setShowArticles(!showArticles)}\n          >\n            {showArticles ? (\n              <p className=\"buttonText\">Hide Articles</p>\n            ) : (\n              <p className=\"buttonText\">Show Articles</p>\n            )}\n          </button>\n          <ul>\n            {showArticles ? (\n              articlesLoading ? (\n                <Loading item=\"articles\" />\n              ) : (\n                articles.map((article) => {\n                  return (\n                    <UserArticlesCard key={article.article_id} {...article} />\n                  );\n                })\n              )\n            ) : (\n              <></>\n            )}\n          </ul>\n        </li>\n        <li>\n          {commentsLoading\n            ? 'Calculating comments... '\n            : `${comments.length} comments. `}\n          <button\n            type=\"button\"\n            className=\"userButton\"\n            onClick={() => setShowComments(!showComments)}\n          >\n            {showComments ? (\n              <p className=\"buttonText\">Hide Comments</p>\n            ) : (\n              <p className=\"buttonText\">Show Comments</p>\n            )}\n          </button>\n          <ul>\n            {showComments ? (\n              commentsLoading ? (\n                <Loading item=\"comments\" />\n              ) : (\n                comments.map((comment, i) => {\n                  return (\n                    <UserCommentsCard\n                      key={comment.comment_id}\n                      {...comment}\n                      index={i}\n                      setComments={setComments}\n                      comments={comments}\n                    />\n                  );\n                })\n              )\n            ) : (\n              <></>\n            )}\n          </ul>\n        </li>\n      </ul>\n    </section>\n  );\n};\n\nexport default UserCard;\n","/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/UserArticlesCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/UserCommentsCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/Utils/date-format.js",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/SearchResults.jsx",["114"],"import React, { useState, useEffect } from 'react';\nimport SearchResultsCard from './SearchResultsCard';\nimport Loading from './Loading';\nimport { fetchSearchedContent } from './Api';\n\nconst SearchResults = ({ option, term }) => {\n  const [content, setContent] = useState({});\n  const [loading, setLoading] = useState(true);\n\n  useEffect(() => {\n    fetchSearchedContent(option, term)\n      .then((response) => {\n        return response;\n      })\n      .then((result) => {\n        setContent(result);\n        setLoading(false);\n      });\n  }, [option]);\n\n  return loading ? (\n    <Loading item={option} />\n  ) : (\n    <section id={option}>\n      <SearchResultsCard key={term} content={content} option={option} />\n    </section>\n  );\n};\n\nexport default SearchResults;\n","/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/SearchResultsCard.jsx",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/Utils/query-format.js",[],"/Users/yunadanta/Desktop/Northcoders/Frontend/nc-news/nc-news/src/Components/SearchConvert.jsx",["115"],"import React, { useState, useEffect } from 'react';\nimport { formatQuery } from './Utils/query-format';\nimport { fetchArticles } from './Api';\nimport SearchResults from './SearchResults';\n\nconst SearchConvert = (props) => {\n  const [newTerm, setNewTerm] = useState('');\n  const [changedTerm, setChangedTerm] = useState(false);\n\n  const query = formatQuery(props.location.search);\n\n  const { option, term } = query;\n\n  useEffect(() => {\n    fetchArticles()\n      .then((response) => {\n        return response;\n      })\n      .then((data) => {\n        return data.filter((article) => article.title === term);\n      })\n      .then((result) => {\n        return result.length > 0 ? result[0].article_id : term;\n      })\n      .then((alteredTerm) => {\n        setNewTerm(alteredTerm);\n        setChangedTerm(true);\n      });\n  }, []);\n